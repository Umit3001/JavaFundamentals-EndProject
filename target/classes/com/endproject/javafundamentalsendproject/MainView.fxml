<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="borderPane" prefHeight="705.0" prefWidth="1014.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.endproject.javafundamentalsendproject.Controller.MainController">
    <left>
        <VBox fx:id="navigationVBox" prefHeight="507.0" prefWidth="190.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="dashboardButton" mnemonicParsing="false" onAction="#onDashBoardButtonClick" prefHeight="26.0" prefWidth="197.0" text="Dashboard">
                    <VBox.margin>
                        <Insets bottom="7.0" left="10.0" right="10.0" top="60.0" />
                    </VBox.margin>
                </Button>
                <Button fx:id="createOrderButton" mnemonicParsing="false" onAction="#onCreateOrderButtonClick" prefHeight="26.0" prefWidth="194.0" text="Create order">
                    <VBox.margin>
                        <Insets bottom="7.0" left="10.0" right="10.0" />
                    </VBox.margin>
                </Button>
                <Button fx:id="productInventoryButton" mnemonicParsing="false" onAction="#onProductInventoryButtonClick" prefHeight="26.0" prefWidth="184.0" text="Product inventory">
                    <VBox.margin>
                        <Insets bottom="7.0" left="10.0" right="10.0" />
                    </VBox.margin>
                </Button>
                <Button fx:id="orderHistoryButton" mnemonicParsing="false" onAction="#onOrderHistoryButtonClick" prefHeight="26.0" prefWidth="172.0" text="Order history">
                    <VBox.margin>
                        <Insets bottom="7.0" left="10.0" right="10.0" />
                    </VBox.margin>
                </Button>
            </children>
        </VBox>
    </left>
   <center>
      <VBox fx:id="pageVBox" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
